<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeOneVM" type="com.moeezasapplication.app.modules.homeone.data.viewmodel.HomeOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearHomeOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylered_50">

        <LinearLayout android:id="@+id/linearTopbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageMenu"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_28pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_menu"
                tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
            <ImageView android:id="@+id/imageNotification"
                android:layout_height="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_261pxh"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_notification"
                tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowgoodevening"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumngoodevening"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_weight="0.8" android:layout_width="0dp" android:orientation="vertical">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGoodEvening"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtGoodEvening}"
                    style="@style/txtUrbanistromanmedium16_4" tools:text="@string/lbl_good_evening"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEmmanuel"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_6pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtEmmanuel}"
                    style="@style/txtUrbanistromanbold28" tools:text="@string/lbl_emmanuel"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageRectangleTwelve"
                android:layout_height="@dimen/_64pxh"
                android:layout_width="@dimen/_64pxh"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_12pxh}"
                imageRes="@{@drawable/img_rectangle12}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle12"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtSwipetotaket"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="wrap_content"
            android:lineSpacingExtra="@dimen/_3pxh"
            android:singleLine="true"
            android:text="@{homeOneVM.homeOneModel.txtSwipetotaket}"
            style="@style/txtUrbanistromanmedium14" tools:text="@string/msg_swipe_to_take_t"/>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearNewtieHelper"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylepink_501cornerRadius">
            <ImageView android:id="@+id/imageQuestion"
                android:layout_height="@dimen/_28pxh"
                android:layout_marginBottom="@dimen/_38pxv"
                android:layout_marginStart="@dimen/_33pxh"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="@dimen/_46pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_question"
                tools:ignore="ContentDescription" tools:src="@drawable/img_question"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearColumnclose"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginStart="@dimen/_30pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_weight="0.83" android:layout_width="0dp" android:orientation="vertical">
                <ImageView android:id="@+id/imageClose"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginEnd="@dimen/_3pxh"
                    android:layout_marginStart="@dimen/_210pxh"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_close"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtWelcometoDoNa"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_68pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtWelcometoDoNa}"
                    style="@style/txtUrbanistromanbold16" tools:text="@string/msg_welcome_to_dona"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtHelloEmmanuel"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:text="@{homeOneVM.homeOneModel.txtHelloEmmanuel}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/msg_hello_emmanuel"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowsend"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnsend"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyleindigo_51cornerRadius">
                <ImageButton
                    android:background="@drawable/rectangle_bg_indigo_52_radius_35"
                    android:id="@+id/btnSend"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_70pxh"
                    android:layout_marginEnd="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:paddingHorizontal="@dimen/_7pxh"
                    android:paddingVertical="@dimen/_7pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_send" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWardrobe"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtWardrobe}"
                    style="@style/txtUrbanistromanbold16_1" tools:text="@string/lbl_wardrobe"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnmusic"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylegray_100cornerRadius">
                <ImageButton
                    android:background="@drawable/rectangle_bg_teal_50_radius_35"
                    android:id="@+id/btnMusic"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_70pxh"
                    android:layout_marginEnd="@dimen/_18pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:paddingHorizontal="@dimen/_7pxh"
                    android:paddingVertical="@dimen/_7pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_music" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtDonateClothes"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_17pxh"
                    android:layout_marginStart="@dimen/_18pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtDonateClothes}"
                    style="@style/txtUrbanistromanbold16_2" tools:text="@string/lbl_donate_clothes"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowchallenges"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnstarthree"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStylegray_100_abcornerRadius">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowstarthree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="wrap_content"
                    android:orientation="horizontal" style="@style/groupStyleblue_50cornerRadius">
                    <ImageView android:id="@+id/imageStarThree"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_23pxv"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_34pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star3"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star3"/>
                    <ImageView android:id="@+id/imageStarOne"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_23pxv"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star1"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star1"/>
                    <ImageView android:id="@+id/imageStarTwo"
                        android:layout_height="@dimen/_13pxh"
                        android:layout_marginBottom="@dimen/_23pxv"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_34pxv"
                        android:layout_width="@dimen/_13pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_star2"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_star2"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="center_horizontal"
                    android:id="@+id/txtChallenges"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_18pxv"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="match_parent"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtChallenges}"
                    style="@style/txtUrbanistromanbold16_3" tools:text="@string/lbl_challenges"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnclock"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:orientation="vertical" style="@style/groupStyleorange_50cornerRadius">
                <ImageView android:id="@+id/imageClock"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_70pxh"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="@dimen/_70pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtReminder"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_37pxh"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{homeOneVM.homeOneModel.txtReminder}"
                    style="@style/txtUrbanistromanbold16_4" tools:text="@string/lbl_reminder"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameStackvolume"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_29pxv"
            android:layout_marginBottom="@dimen/_44pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_84pxv" android:layout_width="@dimen/_312pxh">
            <LinearLayout android:id="@+id/linearRowvolume"
                android:layout_gravity="end"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_63pxh"
                android:layout_marginStart="@dimen/_63pxh"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <ImageView android:id="@+id/imageVolume"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_volume"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_volume"/>
                <ImageView android:id="@+id/imageLocation"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_location_black_903"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_location_black_903"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowuser"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home_gray_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home_gray_900"/>
                    <ImageView android:id="@+id/imageLocationOne"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_47pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_location_gray_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_location_gray_900"/>
                    <ImageView android:id="@+id/imageVolumeOne"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_46pxh"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_volume_gray_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_volume_gray_900"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_121pxh"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_gray_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_gray_900"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
