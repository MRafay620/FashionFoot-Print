<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="helpVM" type="com.moeezasapplication.app.modules.help.data.viewmodel.HelpVM"/>
    </data>
    <FrameLayout android:id="@+id/frameHelp"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylepink_50">
        <LinearLayout android:id="@+id/linearColumnleftside"
            android:layout_gravity="top|center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent" android:orientation="vertical">

            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_border_white_a700_radius_12"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_41pxh"
                    android:layout_width="@dimen/_41pxh"
                    android:paddingHorizontal="@dimen/_13pxh"
                    android:paddingVertical="@dimen/_13pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHelpSupport"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{helpVM.helpModel.txtHelpSupport}"
                    style="@style/txtIntersemibold30" tools:text="@string/lbl_help_support"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowinfo"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageInfo"
                    android:layout_height="@dimen/_45pxh"
                    android:layout_width="@dimen/_45pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_info_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_info_black_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAboutus"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{helpVM.helpModel.txtAboutus}"
                    style="@style/txtInterregular32" tools:text="@string/lbl_about_us"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowinfoOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_62pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageInfoOne"
                    android:layout_height="@dimen/_43pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_43pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_info_black_900_43x43"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_info_black_900_43x43"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrivacyPolic"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.85"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{helpVM.helpModel.txtPrivacyPolic}"
                    style="@style/txtInterregular32" tools:text="@string/msg_privacy_polic"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowminimize"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_7pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_82pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageMinimize"
                    android:layout_height="@dimen/_41pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_minimize_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_minimize_black_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTermConditio"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{helpVM.helpModel.txtTermConditio}"
                    style="@style/txtInterregular32" tools:text="@string/msg_term_conditio"/>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
