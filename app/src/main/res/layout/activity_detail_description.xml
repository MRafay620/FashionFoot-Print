<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="detailDescriptionVM" type="com.moeezasapplication.app.modules.detaildescription.data.viewmodel.DetailDescriptionVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearDetaildescript"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylepink_50">
        <FrameLayout android:id="@+id/frameStackleftside"
            android:layout_height="@dimen/_387pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImageTwelve"
                android:layout_gravity="start"
                android:layout_height="@dimen/_387pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image12_387x375}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image12_387x375"/>
            <LinearLayout android:id="@+id/linearColumnleftside"
                android:layout_gravity="top|center"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent" android:orientation="vertical">

                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_border_white_a700_radius_12"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_41pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="@dimen/_41pxh"
                    android:paddingHorizontal="@dimen/_13pxh"
                    android:paddingVertical="@dimen/_13pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumncasualwear"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_5">
            <TextView android:ellipsize="end"
                android:id="@+id/txtCasualWear"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{detailDescriptionVM.detailDescriptionModel.txtCasualWear}"
                style="@style/txtIntersemibold30" tools:text="@string/lbl_casual_wear"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAboutOutfit"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_marginStart="@dimen/_22pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{detailDescriptionVM.detailDescriptionModel.txtAboutOutfit}"
                style="@style/txtIntermedium18" tools:text="@string/lbl_about_outfit"/>
            <TextView android:gravity="end"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:letterSpacing="0.04"
                android:text="@{detailDescriptionVM.detailDescriptionModel.txtDescription}"
                style="@style/txtInterregular14" tools:text="@string/msg_these_soft_cak"/>
            <FrameLayout android:id="@+id/frameStackhome"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_31pxv"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_147pxv" android:layout_width="@dimen/_312pxh">
                <LinearLayout android:id="@+id/linearRowuser"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowhome"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_weight="0.88"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageHome"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_home_black_911"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_home_black_911"/>
                        <ImageView android:id="@+id/imageFavourite"
                            android:layout_height="@dimen/_25pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_47pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_25pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_favourite"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_favourite"/>
                        <ImageView android:id="@+id/imageVolume"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginStart="@dimen/_46pxh"
                            android:layout_width="@dimen/_23pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_volume_black_913"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_volume_black_913"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageUser"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_121pxh"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_black_914"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_black_914"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageLocation"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginEnd="@dimen/_59pxh"
                    android:layout_marginStart="@dimen/_59pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_location_black_915"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_location_black_915"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
