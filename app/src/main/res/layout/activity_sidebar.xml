<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="sidebarVM" type="com.moeezasapplication.app.modules.sidebar.data.viewmodel.SidebarVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearSidebar"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_34pxh"
            android:layout_marginStart="@dimen/_34pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowgroupfive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageGroupFive"
                    android:layout_height="@dimen/_72pxh"
                    android:layout_width="@dimen/_72pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group5"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_group5"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnfashion"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_weight="0.53"
                    android:layout_width="0dp" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFashion"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_5pxh"
                        android:singleLine="true"
                        android:text="@{sidebarVM.sidebarModel.txtFashion}"
                        style="@style/txtUrbanistromanbold24" tools:text="@string/lbl_fashion"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFootprint"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:singleLine="true"
                        android:text="@{sidebarVM.sidebarModel.txtFootprint}"
                        style="@style/txtUrbanistromanmedium16_2" tools:text="@string/lbl_footprint"/>
                </LinearLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowinfo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageInfo"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_24pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_info"
                tools:ignore="ContentDescription" tools:src="@drawable/img_info"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAccount"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.71" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtAccount}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/lbl_account"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowstar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageStar"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_star"
                tools:ignore="ContentDescription" tools:src="@drawable/img_star"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDonateClothes"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.81" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtDonateClothes}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/lbl_donate_clothes"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowsettings"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_39pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageSettings"
                android:layout_height="@dimen/_24pxh"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_settings"
                tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtInviteAFriend"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_weight="0.8" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtInviteAFriend}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/lbl_invite_a_friend"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowalerttriangle"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageAlerttriangle"
                android:layout_height="@dimen/_18pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_alerttriangle"
                tools:ignore="ContentDescription" tools:src="@drawable/img_alerttriangle"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtReportAProble"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.86" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtReportAProble}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/msg_report_a_proble"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowapperanceicon"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageApperanceicon"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_25pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_apperanceicon"
                tools:ignore="ContentDescription" tools:src="@drawable/img_apperanceicon"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAppearance"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.78" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtAppearance}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/lbl_appearance"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowicbaselinesup"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_36pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageIcbaselinesup"
                android:layout_height="@dimen/_22pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="@dimen/_23pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_icbaselinesup"
                tools:ignore="ContentDescription" tools:src="@drawable/img_icbaselinesup"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtHelpSupport"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_weight="0.82" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtHelpSupport}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/lbl_help_support"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowsettingsOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_242pxv"
            android:layout_marginEnd="@dimen/_32pxh"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_37pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageSettingsOne"
                android:layout_height="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_settings_black_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_settings_black_900"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSettingsTwo"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.72" android:layout_width="0dp"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:singleLine="true"
                android:text="@{sidebarVM.sidebarModel.txtSettingsTwo}"
                style="@style/txtUrbanistromanmedium16_3" tools:text="@string/lbl_settings"/>
        </LinearLayout>
    </LinearLayout>
</layout>
