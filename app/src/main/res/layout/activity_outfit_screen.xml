<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="outfitScreenVM" type="com.moeezasapplication.app.modules.outfitscreen.data.viewmodel.OutfitScreenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearOutfitScreen"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylepink_50">

        <ImageButton
            android:background="@drawable/rectangle_bg_white_a700_border_white_a700_radius_12"
            android:id="@+id/btnArrowleft"
            android:layout_height="@dimen/_41pxh"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="@dimen/_41pxh"
            android:paddingHorizontal="@dimen/_13pxh"
            android:paddingVertical="@dimen/_13pxv"
            android:scaleType="fitCenter"
            android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtHeresSuggesti"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:layout_marginStart="@dimen/_22pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{outfitScreenVM.outfitScreenModel.txtHeresSuggesti}"
            style="@style/txtIntersemibold30" tools:text="@string/msg_here_s_suggesti"/>
        <FrameLayout android:id="@+id/frameStackgroup412"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_198pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_8pxv" android:layout_width="@dimen/_332pxh">
            <ImageView android:id="@+id/imageImageTwelve"
                android:layout_gravity="start"
                android:layout_height="@dimen/_198pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image12}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image12"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                android:id="@+id/btnGroup412"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_45pxh"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_45pxh"
                android:paddingHorizontal="@dimen/_9pxh"
                android:paddingVertical="@dimen/_9pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_group412" tools:ignore="ContentDescription"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameStackgroup413"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_235pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_34pxv" android:layout_width="@dimen/_327pxh">
            <ImageView android:id="@+id/imageImageThirteen"
                android:layout_gravity="start"
                android:layout_height="@dimen/_235pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_image13}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_image13"/>
            <ImageButton
                android:background="@drawable/rectangle_bg_white_a700_radius_15"
                android:id="@+id/btnGroup413"
                android:layout_gravity="bottom|end"
                android:layout_height="@dimen/_45pxh"
                android:layout_marginBottom="@dimen/_13pxv"
                android:layout_marginEnd="@dimen/_4pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="@dimen/_45pxh"
                android:paddingHorizontal="@dimen/_9pxh"
                android:paddingVertical="@dimen/_9pxv"
                android:scaleType="fitCenter"
                android:src="@drawable/img_group412" tools:ignore="ContentDescription"/>
        </FrameLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnGetDetails"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_57pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_width="@dimen/_259pxh"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/lbl_get_details"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_get_details"/>
        <FrameLayout android:id="@+id/frameStackhome"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_31pxv"
            android:layout_marginBottom="@dimen/_44pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_31pxv" android:layout_width="@dimen/_312pxh">
            <LinearLayout android:id="@+id/linearRowuser"
                android:layout_gravity="bottom|center"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowhome"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.88"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home_black_906"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home_black_906"/>
                    <ImageView android:id="@+id/imageClock"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_47pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_clock_black_907"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_clock_black_907"/>
                    <ImageView android:id="@+id/imageVolume"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_46pxh"
                        android:layout_width="@dimen/_23pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_volume_black_908"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_volume_black_908"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_26pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_121pxh"
                    android:layout_width="@dimen/_23pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_black_909"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_black_909"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageLocation"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_28pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_61pxh"
                android:layout_marginStart="@dimen/_61pxh"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_location_black_910"
                tools:ignore="ContentDescription" tools:src="@drawable/img_location_black_910"/>
        </FrameLayout>
    </LinearLayout>
</layout>
